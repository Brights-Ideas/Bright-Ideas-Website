      <umbPropertyDetails id="1404" key="4633071c-27e3-46fa-bf4d-397dd5bdfa34" parentID="1402" level="3" creatorID="0" sortOrder="2" createDate="2016-12-13T18:31:55" updateDate="2016-12-14T01:01:25" nodeName="1 Greenhill Street, Evesham" urlName="1-greenhill-street-evesham" path="-1,1401,1402,1404" isDoc="" nodeType="1310" creatorName="Andrew Bright" writerName="Andrew Bright" writerID="0" template="1309" nodeTypeAlias="umbPropertyDetails">
