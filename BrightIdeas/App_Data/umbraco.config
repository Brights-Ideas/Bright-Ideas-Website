      <umbPropertyDetails id="1405" key="7345b5f9-c402-4409-9ba0-39cce33a5b7e" parentID="1402" level="3" creatorID="0" sortOrder="3" createDate="2016-12-13T18:32:00" updateDate="2016-12-14T00:34:42" nodeName="Garnes, Guilthwaite Hill" urlName="garnes-guilthwaite-hill" path="-1,1401,1402,1405" isDoc="" nodeType="1310" creatorName="Andrew Bright" writerName="Andrew Bright" writerID="0" template="1309" nodeTypeAlias="umbPropertyDetails">
